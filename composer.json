{
  "name" : "harentius/blog-bundle",
  "type": "symfony-bundle",
  "description": "Symfony simple blog/cms",
  "keywords": ["Symfony blog cms"],
  "homepage": "https://github.com/harentius/blog-bundle",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Yaroslav Osypenko",
      "email": "yaroslav.osypenko@gmail.com",
      "homepage": "http://folkprog.net"
    }
  ],
  "autoload" : {
    "psr-4": { "Harentius\\BlogBundle\\": "" }
  },
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": ">=2.7",
    "stof/doctrine-extensions-bundle": ">=1.1",
    "sonata-project/admin-bundle": ">=2.3",
    "fxp/composer-asset-plugin": ">=1.0.0",
    "bower-asset/bootstrap": ">3.3",
    "knplabs/knp-paginator-bundle": ">=2.4",
    "hautelook/alice-bundle": ">=0.2",
    "knplabs/knp-menu-bundle": ">=2.0",
    "whiteoctober/breadcrumbs-bundle": ">=1.1.0",
    "sonata-project/doctrine-orm-admin-bundle": ">=2.3",
    "beberlei/DoctrineExtensions": ">=1.0",
    "bower-asset/ckeditor": ">=4.4.7",
    "bower-asset/ckeditor-youtube-plugin": ">=2.0",
    "bower-asset/ckeditor-more-plugin": "dev-master",
    "bower-asset/ckeditor-audio-plugin": ">=1.0.0",
    "bower-asset/jquery": ">=2.1",
    "friendsofsymfony/jsrouting-bundle": ">=1.5.4",
    "bower-asset/components-font-awesome": ">=4.4",
    "presta/sitemap-bundle": ">=1.4",
    "bower-asset/ckeditor-wordcount-plugin": ">=1.1",
    "symfony-cmf/routing-bundle": ">=1.3.5",
    "eko/feedbundle": ">=1.2.6",
    "bower-asset/js-cookie": ">=2.0.3",
    "sonata-project/translation-bundle": "1.0.0"
  },
  "extra": {
    "asset-repositories": [
      {
        "type": "bower-vcs",
        "url": "https://github.com/zionsg/CKEditor-More-plugin.git",
        "name": "bower-asset/ckeditor-more-plugin"
      },
      {
        "type": "bower-vcs",
        "url": "https://github.com/w8tcha/CKEditor-WordCount-Plugin.git",
        "name": "bower-asset/ckeditor-wordcount-plugin"
      }
    ]
   }
}
